{
    "name": "sepisoltani/iran",
    "description": "Laravel package that helps Iranian developers to have Iran provinces , countries and cities in their project",
    "license": "MIT",
    "authors": [
        {
            "name": "sepehr",
            "email": "sph_1996@yahoo.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Sepisoltani\\Iran\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sepisoltani\\Iran\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testdox --verbose --colors=always"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4",
        "predis/predis": "~1.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sepisoltani\\Iran\\IranServiceProvider"
            ],
            "aliases": {
                "Iran": "Sepisoltani\\Iran\\Facades\\Iran"
            }
        }
    },
    "config": {
        "discard-changes": true
    },
    "require-dev": {
        "phpunit/phpunit": "8.5",
        "orchestra/testbench": "^6.2",
        "barryvdh/laravel-ide-helper": "^2.8"
    }
}
